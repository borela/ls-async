{
  "name": "ls-async",
  "version": "1.0.7",
  "description": "List directories asynchronously.",
  "main": "build",
  "scripts": {
    "check": "flow check || exit 0",
    "clean": "rimraf \"build/!(.git)\"",
    "build": "run-s build:*",
    "build:clean": "npm run clean",
    "build:target": "webpack --progress --display-error-details",
    "dev": "run-s dev:*",
    "dev:clean": "npm run clean",
    "dev:target": "webpack --progress --watch --display-error-details",
    "dist": "run-s dist:*",
    "dist:clean": "npm run clean",
    "dist:target": "cross-env NODE_ENV=production webpack --progress --display-error-details",
    "start": "npm run dev"
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-export-extensions",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borela/ls-async.git"
  },
  "keywords": [
    "list",
    "directory"
  ],
  "author": [
    {
      "name": "Alexandre Borela",
      "email": "alexandre@borela.tech",
      "url": "http://www.borela.tech"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/borela/ls-async/issues"
  },
  "homepage": "https://github.com/borela/ls-async#readme",
  "dependencies": {
    "ctrine-preset-node": "^1.0.0"
  },
  "devDependencies": {
    "ctrine-webpack-settings-node": "^1.0.0"
  }
}
